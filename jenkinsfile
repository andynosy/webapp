pipeline {
  tools {
    maven 'maven'
  }
  agent any 
  stages {
    stage ('initialize') {
	  steps {
	  
	    sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
            '''   
	  
	  }
	
	}
	
    stage ('build') {
	
	  steps {
	  
	    sh 'mvn clear package'
	  }
	  
	  
	 
  }


}
